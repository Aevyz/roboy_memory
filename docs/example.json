//header and payload are arguments, here they in json just to keep the structure of json document
//create requests
[
  //header example
  {
    "header": {
      "user": "dialog",
      "datetime": "1.07.2017 14:55:11"
    }
  },
  //for node
  {
    "payload": {
      "type": "node",
      "label": "person", //only one label is supported for now
      "properties": { //this is a dictionary
        "age": 100,
        "name": "Alex"
      }
    }
  },
  //for relation
  {
    "payload": {
      "type": "relation",
      "label": "lives_in", //only one label is supported for now
      "properties": { //this is a dictionary
        "from": 9898759375, //REQUIRED, contains node id
        "to": 895743987534, //REQUIRED, contains node id
        "role": "whatever"
      }
    }
  },
  // update requests
  // possible to update properties only
  {
    "payload": {
      "id": 465835873457,
      "type": "relation", //or node
      "properties": { //this is a dictionary
        "role": "whatever1"
      }
    }
  }
]